	<HediffDef ParentName="MSE_AddedBodyPartBionicSpecial_Organ">
		<defName>AntitoxinKidney</defName>
		<label>antitoxin kidney</label>
		<labelNoun>an antitoxin kidney</labelNoun>
		<stages Inherit="False">
			<li>
				<statOffsets>
					<ArmorRating_Blunt>0.002</ArmorRating_Blunt>
					<ArmorRating_Heat>0.002</ArmorRating_Heat>
					<ArmorRating_Sharp>0.002</ArmorRating_Sharp>
					<!----><ToxicSensitivity>-0.25</ToxicSensitivity>
				</statOffsets>
			</li>
			<li>
				<label>adapting</label>
				<minSeverity>0.0001</minSeverity>
				<painOffset>0.05</painOffset>
				<statOffsets>
					<!----><ToxicSensitivity>-0.15</ToxicSensitivity>
				</statOffsets>
			</li>
			<li>
				<label>begin to adapt</label>
				<minSeverity>0.60</minSeverity>
				<partEfficiencyOffset>-0.15</partEfficiencyOffset>
				<painOffset>0.10</painOffset>
				<vomitMtbDays>1.0</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.05</offset>
					</li>
				</capMods>
				<statOffsets>
					<!----><ToxicSensitivity>-0.05</ToxicSensitivity>
				</statOffsets>
			</li>
			<li>
				<label>newly installed</label>
				<minSeverity>0.85</minSeverity>
				<partEfficiencyOffset>-0.30</partEfficiencyOffset>
				<painOffset>0.15</painOffset>
				<vomitMtbDays>0.5</vomitMtbDays>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.15</offset>
					</li>
				</capMods>
			</li>
			<li>
				<label>newly installed</label>
				<minSeverity>0.95</minSeverity>
				<partEfficiencyOffset>-0.30</partEfficiencyOffset>
				<painOffset>0.15</painOffset>
				<capMods>
					<li>
						<capacity>Consciousness</capacity>
						<offset>-0.85</offset>
					</li>
				</capMods>
			</li>
		</stages>
		<spawnThingOnRemoved>AntitoxinKidney</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.00</partEfficiency>
		</addedPartProps>
	</HediffDef>

