		<defName>MSE_EndoskeletonStimulator</defName>
		<label>endoskeleton stimulator</label>
		<labelNoun>an endoskeleton stimulator</labelNoun>
		<stages>
			<li>
				<capMods>
					<li>
						<capacity>Manipulation</capacity>
						<offset>0.15</offset>
					</li>
