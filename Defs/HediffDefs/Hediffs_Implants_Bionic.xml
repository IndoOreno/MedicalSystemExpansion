<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<HediffDef ParentName="MSE_ImplantHediff_Brain">
		<defName>Joywire</defName>
		<label>joywire</label>
		<labelNoun>a joywire</labelNoun>
		<stages>
			<li>
				<partEfficiencyOffset>-0.20</partEfficiencyOffset>
			</li>
		</stages>
		<spawnThingOnRemoved>Joywire</spawnThingOnRemoved>
		<modExtensions>
			<li Class="OrenoMSE.MSE_AdditionalHediff">
				<hediffsToAdd>
					<li>MSE_JoywireComplication</li>
				</hediffsToAdd>
			</li>
		</modExtensions>
	</HediffDef>

